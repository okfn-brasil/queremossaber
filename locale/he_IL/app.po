msgid_plural "%d Freedom of Information requests to %s"
msgstr[0] "%d בקשת תחת חוק חופש המידע עבור %s"
msgstr[1] "%d בקשות תחת חוק חופש המידע עבור %s"

msgid "%d request"
msgid_plural "%d requests"
msgstr[0] "%d בקשה"
msgstr[1] "%d בקשות"

